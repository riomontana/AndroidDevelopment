<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lfo.laboration2b.MainActivity">

    <fragment
        android:id="@+id/fragmentTextView"
        android:name="com.example.lfo.laboration2b.TextViewFragment"
        android:layout_width="163dp"
        android:layout_height="130dp"
        android:layout_marginBottom="209dp"
        app:layout_constraintBottom_toTopOf="@+id/fragmentButton"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout="@layout/fragment_text_view"
        app:layout_constraintHorizontal_bias="0.502" />

    <fragment
        android:id="@+id/fragmentButton"
        android:name="com.example.lfo.laboration2b.ButtonFragment"
        android:layout_width="175dp"
        android:layout_height="75dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout="@layout/fragment_button"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        tools:layout_editor_absoluteY="420dp" />
</android.support.constraint.ConstraintLayout>
